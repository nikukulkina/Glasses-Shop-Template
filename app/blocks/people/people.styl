.people
	margin-top 10px
	margin-bottom 10px
	padding 5px 0
	display flex
	flex-wrap wrap
	height 125px
	border-top 1px solid $light-blue
	border-bottom 1px solid $light-blue

	&__wrap
		display flex
		flex-wrap wrap
		align-items center
		width 100%
		justify-content space-between
		
	@media(max-width 900px)
		&__wrap
			justify-content space-around